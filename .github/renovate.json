{
  "extends": ["config:recommended"],
  "mise": {
    "packageRules": [
      {
        "matchPackageNames": ["awscli"],
        "versionCompatibility": "^(?<compatibility>.*):(?<version>.*)$"
      },
      {
        "matchPackageNames": ["java"],
        "versionCompatibility": "^(?<compatibility>.*?)-(?<version>.*)$"
      }
    ]
  }
}
