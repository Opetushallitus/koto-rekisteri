          <td>{{arvioijanOppijanumero}}</td>
          <td>{{henkilotunnus}}</td>
          <td>{{sukunimi}}</td>
          <td>{{etunimet}}</td>
          <td>{{sahkopostiosoite}}</td>
          <td>{{katuosoite}}, {{postinumero}} {{postitoimipaikka}}</td>
          <td>{{tila}}</td>
          <td>{{kieli}}</td>
          <td>{{tasot}}</td>
          <td>{{#kaudenAlkupaiva}}{{kaudenAlkupaiva}}{{/kaudenAlkupaiva}}</td>
          <td>{{#kaudenPaattymispaiva}}{{kaudenPaattymispaiva}}{{/kaudenPaattymispaiva}}</td>
          <td>{{jatkorekisterointi}}</td>
          <td>{{rekisteriintuontiaika}}</td>
        </tr>
        {{/arvioijat}}
      </tbody>
    </table>
{{/body}}
