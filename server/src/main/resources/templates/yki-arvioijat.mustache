{{/title}}
{{$body}}
    <h1>Yleiset kielitutkinnot - arvioijat</h1>
    <table>
      <thead>
        <tr>
          <th>Oppijanumero</th>
          <th>Henkilötunnus</th>
          <th>Sukunimi</th>
          <th>Etunimet</th>
          <th>Sähköposti</th>
          <th>Osoite</th>
          <th>Tila</th>
          <th>Kieli</th>
          <th>Tasot</th>
          <th>Kauden alkupäivä</th>
          <th>Kauden päättymispäivä</th>
          <th>Jatkorekisteröinti</th>
          <th>Rekisteriintuontiaika</th>
        </tr>
      </thead>
      <tbody>
        {{#arvioijat}}
