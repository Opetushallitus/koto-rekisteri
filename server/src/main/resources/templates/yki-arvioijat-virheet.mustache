{{<page}}
  {{$title}}
    Kielitutkintorekisteri - Yleiset kielitutkinnot - arvioija - virheet
  {{/title}}
  {{$breadcrumbs}}
    <li><a href="/yki/arvioijat">Yleiset kielitutkinnot</a></li>
    <li><a href="/yki/arvioijat">Arvioijat</a></li>
    <li><a href="/yki/suoritukset/virheet">Virheet</a></li>
  {{/breadcrumbs}}
  {{$body}}
    <article class="overflow-auto">
    <table class="compact striped">
      <thead>
      <tr>
        {{#header}}
          <th>
            <a href="?sortColumn={{
              column.lowercaseName
            }}&sortDirection={{
              sortDirection
            }}">
              {{column.uiHeaderValue}} {{symbol}}
            </a>
          </th>
        {{/header}}
      </tr>
      </thead>
      <tbody class="virheet">
      {{#virheet}}
        <tr>
          <td>
              {{#arvioijanOid}}
                  {{arvioijanOid}}
              {{/arvioijanOid}}
              {{^arvioijanOid}}
                 arvo puuttuu
              {{/arvioijanOid}}
          </td>
          <td>
              {{#hetu}}
                  {{hetu}}
              {{/hetu}}
              {{^hetu}}
                  arvo puuttuu
              {{/hetu}}
          </td>
          <td>
              {{#nimi}}
                {{nimi}}
              {{/nimi}}
              {{^nimi}}
                  arvo puuttuu
              {{/nimi}}
          </td>
          <td>
              {{#virheellinenKentta}}
                  {{virheellinenKentta}}
              {{/virheellinenKentta}}
              {{^virheellinenKentta}}
                  arvo puuttuu
              {{/virheellinenKentta}}
          </td>
          <td>
              {{#virheellinenArvo}}
                  {{virheellinenArvo}}
              {{/virheellinenArvo}}
              {{^virheellinenArvo}}
                  arvo puuttuu
              {{/virheellinenArvo}}
          </td>
	      <td>{{virheellinenRivi}}</td>
          <td>{{virheenRivinumero}}</td>
          <td>{{virheenLuontiaika}}</td>
        </tr>
      {{/virheet}}
      </tbody>
    </table>
    </article>
  {{/body}}
{{/page}}
